package model;

import java.util.Date;

public abstract class Document {
	
	private String oggetto;
	private String noprotocollo;
	private Date dataprotocollo;
	private byte[] pdf;
	private byte[] allegato;
	
	public Document(){
		
	}
	
	public Document(String oggetto, String noprotocollo, Date dataprotocollo,
			byte[] pdf, byte[] allegato) {
		super();
		this.setOggetto(oggetto);
		this.setNoprotocollo(noprotocollo);
		this.setDataprotocollo(dataprotocollo);
		this.setPdf(pdf);
		this.setAllegato(allegato);
	}

	public String getOggetto() {
		return oggetto;
	}

	public void setOggetto(String oggetto) {
		this.oggetto = oggetto;
	}

	public String getNoprotocollo() {
		return noprotocollo;
	}

	public void setNoprotocollo(String noprotocollo) {
		this.noprotocollo = noprotocollo;
	}

	public Date getDataprotocollo() {
		return dataprotocollo;
	}

	public void setDataprotocollo(Date dataprotocollo) {
		this.dataprotocollo = dataprotocollo;
	}

	public byte[] getPdf() {
		return pdf;
	}

	public void setPdf(byte[] pdf) {
		this.pdf = pdf;
	}

	public byte[] getAllegato() {
		return allegato;
	}

	public void setAllegato(byte[] allegato) {
		this.allegato = allegato;
	}

	
}
